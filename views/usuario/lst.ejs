<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <style>
        table{
            max-width: 100%;
        }
        table, td, th{
            border: solid 1px red;
            border-collapse:collapse;
        }
        td,th{
            padding: 5px;
        }
    </style>
    <title>Listar Usuários</title>
</head>
<body>
    <table>
        <tr>
            <th>_id</th>
            <th>Nome</th>
            <th>Email</th>
            <th>Foto</th>
            <th>Criação</th>
            <th>Edita</th>
            <th>Deleta</th>
        </tr>

        <%Usuarios.forEach(function(usuario){  %>
            <tr>
                <td><%=usuario._id %></td>
                <td><%=usuario.nome %></td>
                <td><%=usuario.email %></td>
                <td><%=usuario.foto %></td>
                <td><%=usuario.criacao %></td>
                <td>Edita</td>
                <td>Deleta</td>
            </tr>
        <% }) %>
    </table>
</body>
</html>